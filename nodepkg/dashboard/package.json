{
  "name": "@nodepkg/dashboard",
  "version": "0.1.0",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/vuematerial": "^0.7.2",
    "@innoai-tech/vueuikit": "^0.16.1",
    "@nodepkg/runtime": "workspace:*",
    "date-fns": "^4.1.0"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      }
    }
  },
  "files": [
    "dist/*",
    "src/*",
    "!/**/__tests__"
  ],
  "license": "MIT",
  "prettier": {},
  "scripts": {
    "build": "bunx --bun @innoai-tech/monobundle",
    "prepublishOnly": "bun run build",
    "lint": "bunx --bun prettier --write . "
  },
  "type": "module"
}
